{"program":"Bash","groups":[{"title":"Cursor","shortcuts":[{"command":"Move to the start of the current line","steps":[{"type":"key","value":"control"},{"type":"instruction","value":"+"},{"type":"key","value":"a"}]},{"command":"Move backward a character","steps":[{"type":"key","value":"control"},{"type":"instruction","value":"+"},{"type":"key","value":"b"}]},{"command":"Move to the end of the current line","steps":[{"type":"key","value":"control"},{"type":"instruction","value":"+"},{"type":"key","value":"e"}]},{"command":"Move forward a character","steps":[{"type":"key","value":"control"},{"type":"instruction","value":"+"},{"type":"key","value":"f"}]},{"command":"Move ‘forward’ through the history list, fetching the previous command","steps":[{"type":"key","value":"control"},{"type":"instruction","value":"+"},{"type":"key","value":"n"}]},{"command":"Move ‘back’ through the history list, fetching the previous command","steps":[{"type":"key","value":"control"},{"type":"instruction","value":"+"},{"type":"key","value":"p"}]},{"command":"Move back to the start of the current or previous word","steps":[{"type":"key","value":"alt"},{"type":"instruction","value":"+"},{"type":"key","value":"b"}]},{"command":"Move forward to the end of the next word","steps":[{"type":"key","value":"alt"},{"type":"instruction","value":"+"},{"type":"key","value":"f"}]}]},{"title":"Processes","shortcuts":[{"command":"Kill the command you just ran","steps":[{"type":"key","value":"control"},{"type":"instruction","value":"+"},{"type":"key","value":"c"}]},{"command":"Clear the screen","steps":[{"type":"key","value":"control"},{"type":"instruction","value":"+"},{"type":"key","value":"l"}]},{"command":"Send an EOF marker, unless disabled by an option, this will close the current shell (EXIT)","steps":[{"type":"key","value":"control"},{"type":"instruction","value":"+"},{"type":"key","value":"d"}]}]},{"title":"Editing","shortcuts":[{"command":"Clear the screen and redraw the current line, leaving the current line at the top of the screen","steps":[{"type":"key","value":"control"},{"type":"instruction","value":"+"},{"type":"key","value":"l"}]},{"command":"Delete the Word behind the cursor position","steps":[{"type":"key","value":"alt"},{"type":"instruction","value":"+"},{"type":"key","value":"delete"}]},{"command":"Delete the word after the cursor position","steps":[{"type":"key","value":"alt"},{"type":"instruction","value":"+"},{"type":"key","value":"d"}]},{"command":"Delete character under the cursor","steps":[{"type":"key","value":"control"},{"type":"instruction","value":"+"},{"type":"key","value":"d"}]},{"command":"Delete character in front of the cursor","steps":[{"type":"key","value":"control"},{"type":"instruction","value":"+"},{"type":"key","value":"h"}]},{"command":"Cut the word behind the cursor","steps":[{"type":"key","value":"control"},{"type":"instruction","value":"+"},{"type":"key","value":"w"}]},{"command":"Cut the line in front of the the cursor","steps":[{"type":"key","value":"control"},{"type":"instruction","value":"+"},{"type":"key","value":"k"}]},{"command":"Cut everything before the cursor","steps":[{"type":"key","value":"control"},{"type":"instruction","value":"+"},{"type":"key","value":"u"}]},{"command":"Drag the word before point past the word after point, moving point past that word as well","steps":[{"type":"key","value":"alt"},{"type":"instruction","value":"+"},{"type":"key","value":"t"}]},{"command":"Drag the character before the cursor forward over the character at the cursor, moving the cursor forward as well","steps":[{"type":"key","value":"control"},{"type":"instruction","value":"+"},{"type":"key","value":"t"}]},{"command":"Swap the last two words before the cursor","steps":[{"type":"key","value":"escape"},{"type":"instruction","value":"+"},{"type":"key","value":"t"}]},{"command":"Paste the last cut string","steps":[{"type":"key","value":"control"},{"type":"instruction","value":"+"},{"type":"key","value":"y"}]},{"command":"Capitalize